{
  "name": "@uniformdev/uniformconf",
  "version": "18.14.0",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "start": "next start",
    "dev": "run-s context:manifest dev:next",
    "dev:next": "next dev",
    "build": "run-s context:manifest build:next",
    "build:next": "next build",
    "context:manifest": "uniform context manifest download --output ./lib/context/manifest.json",
    "ci:build": "npm run build",
    "uniform:pull": "run-s uniform:pull:canvas uniform:pull:context",
    "uniform:push": "run-s uniform:push:components uniform:push:context uniform:push:compositions uniform:push:compositions:publish",
    "uniform:pull:canvas": "run-s uniform:pull:components uniform:pull:compositions",
    "uniform:pull:compositions": "uniform presentation composition pull ./data/canvas/compositions",
    "uniform:pull:components": "uniform presentation component pull ./data/canvas/components",
    "uniform:push:context:signals": "uniform context signal push ./data/context/signals",
    "uniform:push:context:enrichments": "uniform context enrichment push ./data/context/enrichments",
    "uniform:push:context:aggregates": "uniform context aggregate push ./data/context/aggregates",
    "uniform:push:context:quirks": "uniform context quirk push ./data/context/quirks",
    "uniform:push:context:tests": "uniform context test push ./data/context/tests",
    "uniform:push:context": "run-s uniform:push:context:signals uniform:push:context:enrichments uniform:push:context:quirks uniform:push:context:tests uniform:push:context:aggregates",
    "uniform:pull:context:signals": "uniform context signal pull ./data/context/signals",
    "uniform:pull:context:enrichments": "uniform context enrichment pull ./data/context/enrichments",
    "uniform:pull:context:aggregates": "uniform context aggregate pull ./data/context/aggregates",
    "uniform:pull:context:quirks": "uniform context quirk pull ./data/context/quirks",
    "uniform:pull:context:tests": "uniform context test pull ./data/context/tests",
    "uniform:pull:context": "run-s uniform:pull:context:signals uniform:pull:context:enrichments uniform:pull:context:quirks uniform:pull:context:tests uniform:pull:context:aggregates",
    "uniform:pull:project-map:definitions": "uniform project-map definition pull ./data/project-map/project-map-definitions",
    "uniform:pull:project-map:nodes": "uniform project-map node pull ./data/project-map/project-map-nodes",
    "uniform:pull:project-map": "run-s uniform:pull:project-map:definitions uniform:pull:project-map:nodes",
    "uniform:push:compositions": "uniform presentation composition push ./data/canvas/compositions",
    "uniform:push:compositions:publish": "uniform canvas composition publish --all",
    "uniform:push:components": "uniform presentation component push ./data/canvas/components",
    "uniform:push:project-map:definitions": "uniform project-map definition push ./data/project-map/project-map-definitions",
    "uniform:push:project-map:nodes": "uniform project-map node push ./data/project-map/project-map-nodes",
    "uniform:push:project-map": "run-s uniform:push:project-map:definitions uniform:push:project-map:nodes",
    "package": "run-s package:canvas package:context package:project-map",
    "package:project-map": "run-s package:project-map-definitions package:project-map-nodes",
    "package:project-map-definitions": "uniform project-map definition pull ./data/uniformconf.json",
    "package:project-map-nodes": "uniform project-map node pull ./data/uniformconf.yaml",
    "package:canvas": "run-s package:components package:compositions",
    "package:compositions": "uniform presentation composition pull ./data/uniformconf.yaml",
    "package:components": "uniform presentation component pull ./data/uniformconf.yaml",
    "package:context:signals": "uniform context signal pull ./data/uniformconf.yaml",
    "package:context:enrichments": "uniform context enrichment pull ./data/uniformconf.yaml",
    "package:context:aggregates": "uniform context aggregate pull ./data/uniformconf.yaml",
    "package:context:quirks": "uniform context quirk pull ./data/uniformconf.yaml",
    "package:context:tests": "uniform context test pull ./data/uniformconf.yaml",
    "package:context": "run-s package:context:signals package:context:enrichments package:context:quirks package:context:tests package:context:aggregates",
    "start:stackblitz": "npm run dev:next"
  },
  "dependencies": {
    "@analytics/google-analytics": "^1.0.5",
    "@react-hook/window-scroll": "^1.3.0",
    "@uniformdev/canvas": "18.24.0",
    "@uniformdev/canvas-next": "18.24.0",
    "@uniformdev/canvas-react": "18.24.0",
    "@uniformdev/context": "18.24.0",
    "@uniformdev/context-devtools": "18.24.0",
    "@uniformdev/context-gtag": "18.24.0",
    "@uniformdev/context-next": "18.24.0",
    "@uniformdev/context-react": "18.24.0",
    "@uniformdev/project-map": "18.24.0",
    "analytics": "^0.8.1",
    "cookie": "^0.5.0",
    "next": "^13.1.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/cookie": "^0.4.1",
    "@types/node": "^16.3.3",
    "@types/react": "^18.0.26",
    "@uniformdev/cli": "18.24.0",
    "autoprefixer": "^10.4.2",
    "dotenv": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.8",
    "prettier": "^2.3.2",
    "sass": "^1.49.7",
    "tailwindcss": "^3.2.6",
    "typescript": "^4.9.5"
  },
  "stackblitz": {
    "startCommand": "npm run start:stackblitz",
    "compileTrigger": "save"
  }
}
