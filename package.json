    "@uniformdev/context": "18.6.0",
    "@uniformdev/context-devtools": "18.6.0",
    "@uniformdev/context-gtag": "18.6.0",
    "@uniformdev/context-next": "18.6.0",
    "@uniformdev/context-react": "18.6.0",
    "analytics": "^0.7.5",
    "cookie": "^0.4.2",
    "next": "^13.1.2",
    "nookies": "^2.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
